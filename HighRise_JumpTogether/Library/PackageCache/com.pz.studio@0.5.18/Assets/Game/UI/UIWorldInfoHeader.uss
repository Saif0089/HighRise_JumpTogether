.world-info-header-cell {
    margin-left: -8px;
    margin-right: -8px;
}

.world-info-header-cell__content {
    width: 100%;
    height: 64px;
    background-color: var(--color-fishbase-catfish);
    padding-right: 12px;
}

.rtl .world-info-header-cell__content {
    padding-right: 0;
    padding-left: 12px;
}

.world-info-header-cell Image {
    width: 36px;
    height: 36px;
    background-color: var(--color-fishbase-tuna);
    border-radius: 12px;
    padding: 6px;
    --unity-image-tint-color: var(--color-fishbase-bonito);
    margin-top: auto;
    margin-bottom: auto;
}

.world-info-header-cell .icon-share {
    margin-left: 12px;
    margin-right: 8px;
}

.rtl .world-info-header-cell .icon-share {
    margin-left: 8px;
    margin-right: 12px;
}

.world-info-header-cell .icon-report {

}

.world-info-header-cell .icon-info {
    display: none;
    margin-right: 4px;
}

.rtl .world-info-header-cell .icon-info {
    margin-left: 4px;
    margin-right: 0;
}

.world-info-header-cell__details {
    margin-top: auto;
    margin-bottom: auto;
    margin-right: auto;
    margin-left: 16px;
}

.rtl .world-info-header-cell__details {
    margin-right: 16px;
    margin-left: auto;
}

.world-info-header-cell #_worldName {
    padding: 0;
    margin: 0;
    margin-bottom: 4px;
    color: var(--color-fishbase-whitefish);
    font-size: 15px;
    -unity-font-definition: var(--font-bold);
    text-overflow: ellipsis; overflow: hidden; white-space: nowrap;
}

.world-info-header-cell #_worldOwner {
    padding: 0;
    margin: 0;
    color: var(--color-fishbase-mackerel);
    font-size: 13px;
    text-overflow: ellipsis; overflow: hidden; white-space: nowrap;
}