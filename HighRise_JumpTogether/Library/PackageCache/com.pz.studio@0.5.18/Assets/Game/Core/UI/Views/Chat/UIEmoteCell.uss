.emote-cell {
    width: 66px;
    height: 66px;
    margin-right: 0px;
}

.emote-cell Image {
    overflow: hidden;
    border-radius: 8px;
    width: 100%;
    height: 100%;
}

.emote-cell:checked Image {
    border-color: var(--color-accents-primary);   
    border-width: 3px;
    border-radius: 11px;
}

/* Mythical background */

.emote-cell UIMythicalBackground {
    display: none;
}

.emote-cell .mythical-background-mask {
    display: none;
}

.emote-cell.rarity-mythical UIMythicalBackground {
    display: flex;
}
